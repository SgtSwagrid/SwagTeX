% https://www.overleaf.com/learn/latex/Using_colors_in_LaTeX

% Package definition

\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{SwagTeX-colours}

% Dependencies

\RequirePackage[dvipsnames]{xcolor}

% Commands

\let \col \textcolor

% Text
\colorlet {text} {Black}
\colorlet {quote} {darkgray}

% Collections
\colorlet {collection} {YellowGreen}
\colorlet {domain} {Goldenrod!50!Black}
\colorlet {array} {Blue!70!Black}
\colorlet {element} {SeaGreen}

% Graphs
\colorlet {graph} {JungleGreen}
\colorlet {vertex} {BrickRed!90!Orchid}
\colorlet {edge} {NavyBlue!90!Orchid}
\colorlet {mixed} {Orchid!90!NavyBlue}
\colorlet {graphop} {JungleGreen!40!black}

% Functions
\colorlet {function} {BurntOrange!80!white!80!graphop}
\colorlet {aux} {Gray}